{
    "id": "FlexModel",
    "modules": [
        {
            "module_id": "Ag1Com",
            "type": "local_broadcast"
        },
        {
            "module_id": "BaselineMPC",
            "type": "agentlib_mpc.mpc",
            "optimization_backend": {
                "type": "casadi",
                "model": {
                    "type": {
                        "file": "mpc_and_sim/simple_model.py",
                        "class_name": "BaselineMPCModel"
                    }
                },
                "discretization_options": {
                    "collocation_order": 2,
                    "collocation_method": "legendre"
                },
                "solver": {
                    "name": "ipopt",
                    "options": {
                        "ipopt": {
                            "max_iter": 30,
                            "tol": 1e-5
                        }
                    }
                },
                "results_file": "results/mpc_simple_building.csv",
                "save_results": true,
                "overwrite_result_file": true
            },
            "time_step": 900,
            "prediction_horizon": 48,
            "parameters": [
                {
                    "name": "s_T", 
                    "value": 250
                },
                {
                    "name": "r_pel", 
                    "value": 1
                }
            ],
            "inputs": [
                {
                    "name": "T_upper", 
                    "value": 299, 
                    "interpolation_method": "previous"
                },
                {
                    "name": "T_lower", 
                    "value": 290, 
                    "interpolation_method": "previous"
                },
                {
                    "name": "T_amb", 
                    "value": 278.15
                }
            ],
            "controls": [
                {
                    "name": "P_in", 
                    "value": 100, 
                    "ub": 200, 
                    "lb": 0
                }
            ],
            "states": [
                {
                    "name": "T_zone", 
                    "value": 294.15, 
                    "ub": 303.15, 
                    "lb": 288.15
                }
            ],
            "outputs": [
                {
                    "name": "P_el",
                    "alias": "P_el"
                }
            ]
        }
    ]
}