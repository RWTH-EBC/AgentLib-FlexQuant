{
    "prep_time": 1000,
    "flex_event_duration": 1000,
    "market_time": 1000,
    "weights": [{"name": "s_P", "value": 10}],
    "pos_flex_cost_function": "sum([self.s_T * self.T_slack ** 2, self.s_P * self.P_el])",
    "neg_flex_cost_function": "sum([self.s_T * self.T_slack ** 2, -self.s_P * self.P_el])",
    "standard_cost_function": "sum([self.r_mDot * self.mDot, self.s_T * self.T_slack ** 2])",

    "acceptable_flexibility_tolerance": 0,
    "profile_deviation_weight": 1,
    "random_seed": 1,
    "pos_neg_rate": 1,
    "offer_acceptance_rate": 1,
    "minimum_average_flex": 100,
    "maximum_time_flex": 0,
    "cooldown_timesteps": 2,
    "power_variable": "P_el",
    "delete_files": true,
    "indicator_config": "flexibility_indicators.json",
    "market_config": "flexibility_market.json",
    "path_to_flex_files": "../../FlexibilityQuantification"

}